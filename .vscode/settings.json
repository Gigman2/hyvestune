{
    "editor.formatOnSave": false,
    "[typescript]": {
        "editor.formatOnSave": true
    },
    "eslint.alwaysShowStatus": true,
    "editor.fontLigatures": true,
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": true
    },
    "eslint.validate": [
        "javascript",
        "typescript"
    ],
    "cSpell.words": [
        "autopopulate"
    ],
    "typescript.tsdk": "node_modules/typescript/lib",
}